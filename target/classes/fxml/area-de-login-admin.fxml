<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="124.0" prefWidth="223.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lp2reconhecimentofacial.InterfaceController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Insira sua senha" />
   <TextField fx:id="passwordField"/>
    <Label fx:id="errorLabel" text="" textFill="red" />
   <Button mnemonicParsing="false" onAction="#autenticateAdmin" text="Enviar" />
    <Button fx:id="exitButton" cancelButton="true" mnemonicParsing="false" onAction="#goBackToMain" text="Voltar" />
</VBox>
