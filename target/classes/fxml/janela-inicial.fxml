<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="124.0" prefWidth="223.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.reconhecimento_facial_lp2.javafx.InterfaceController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label alignment="CENTER" prefHeight="18.0" prefWidth="246.0" text="Reconhecimento Facial - LP2" />
    <Button fx:id="adminLoginButton" onAction="#switchToAdminLoginPage" alignment="CENTER" mnemonicParsing="false" text="Entrar como Admin" />
    <Button fx:id="userLoginButton" onAction="#switchToUserArea" mnemonicParsing="false" text="Entrar como usuario" />
    <Button fx:id="exitButton" onAction="#onExitButtonClick" cancelButton="true" mnemonicParsing="false" text="Sair" />
</VBox>
